{
    "cSpell.enableFiletypes": [
        "shellscript",
        "toml",
    ],
    "cSpell.words": [
        "addopts",
        "APFS",
        "ASCIIbetical",
        "confusables",
        "docstrings",
        "EACCES",
        "EBADF",
        "EFAULT",
        "EINVAL",
        "EIO",
        "EISDIR",
        "elif",
        "ELOOP",
        "ENAMETOOLONG",
        "ENOATTR",
        "ENOENT",
        "ENOENTWIVES",
        "ENOTDIR",
        "ENOTSUP",
        "EOVERFLOW",
        "EPERM",
        "ERANGE",
        "froms",
        "getxattr",
        "libc",
        "mypy",
        "popleft",
        "pytest",
        "readdir",
        "readexceptions",
        "scandir",
        "testpaths",
        "typeshed",
        "xattr"
    ],
    "editor.rulers": [
        90
    ],
    "editor.unicodeHighlight.allowedCharacters": {
        "›": true,
        "–": true,
        "’": true
    },
    "files.exclude": {
        "**/__pycache__": true,
        ".mypy_cache": true,
        ".pytest_cache": true,
        ".ruff_cache": true,
        "coverage": true,
        ".coverage": true
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.env.osx": {
        "PATH": "${env:PATH}:scripts/",
    },
    "python.analysis.extraPaths": [
        "./source"
    ]
}
